{
  "$jsonSchema": {
    "bsonType": "object",
    "required": [
      "name",
      "description",
      "videoURL",
      "targetMuscles",
      "equipmentNeeded",
      "instructions",
      "time",
      "log"
    ],
    "properties": {
      "_id": { "bsonType": "objectId" },
      "name": { "bsonType": "string" },
      "description": { "bsonType": "string" },
      "videoURL": { "bsonType": "string" },
      "targetMuscles": {
        "bsonType": "array",
        "items": { "bsonType": "string" }
      },
      "equipmentNeeded": {
        "bsonType": "array",
        "items": { "bsonType": "string" }
      },
      "instructions": {
        "bsonType": "array",
        "items": { "bsonType": "string" }
      },
      "time": { "bsonType": "int" },
      "log": {
        "bsonType": "object",
        "required": ["proposedReps"],
        "properties": {
          "proposedReps": { "bsonType": "int" },
          "actualReps": { "bsonType": "int", "description": "can be null" },
          "proposedWeight": {
            "bsonType": "double",
            "description": "can be null"
          },
          "actualWeight": { "bsonType": "double", "description": "can be null" }
        }
      }
    }
  }
}
